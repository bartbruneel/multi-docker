{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "bartbruneel/multi-client",
			"hostname": "client",
			"essential": false

		}, {
			"name": "nginx",
			"image": "bartbruneel/multi-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["server","client"]
		}, {
			"name" : "server",
			"image" : "bartbruneel/multi-server",
			"hostname": "api",
			"essential": false
		}, {
			"name": "worker",
			"image": "bartbruneel/multi-worker",
			"hostname":"worker",
			"essential": false
		}
	]
}